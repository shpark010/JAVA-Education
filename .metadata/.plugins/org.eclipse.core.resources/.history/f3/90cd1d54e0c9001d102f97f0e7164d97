class Zcar {
	String color;
	String gearType;
	int door;
	
	Zcar() {	//기본 설정
		this.color = "red";
		this.gearType = "auto";
		this.door = 2;
	}
	
	Zcar(String color, String gearType, int door) {
		this.color = color;
		this.gearType = gearType;
		this.door = door;
	}
	void socarPrint() {
		System.out.println(this.color + " , " +  this.gearType + " , " + this.door);
	}
}

