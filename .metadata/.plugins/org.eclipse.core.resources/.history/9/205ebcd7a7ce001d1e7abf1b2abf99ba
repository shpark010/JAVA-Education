package kr.or.kosa.dto;

public class Lnumber {

	public static void main(String[] args) {
		int[] lotto = new int[6];
		
		for (int i = 0; i < lotto.length; i++) {
			int random = (int)(Math.random()*45)+1;
			boolean checking = false;
			for (int j = 0; j < i; j++) {
				if(lotto[j] == random) {
					checking = true;
					break;
				}
			}
			if(checking) {
				i--;
			}else {
				lotto[i] = random;
			}
		}
		
		
		
		for ( int i : lotto) {
			System.out.print(i + " ");
		}
	}

}
