
class Product {
	int price;
	int bonuspoint;
	
	Product(int price) {
		this.price = price;
		this.bonuspoint = (int)(this.price/10.0);
	}
}

class KtTv extends Product {
	KtTv(int price) {
		super(5000);
	}

	@Override
	public String toString() {
		return "KtTv";
	}
	
}

class Audio extends Product {
	Audio(int price) {
		super(100);
	}
	
	@Override
	public String toString() {
		return "Audio";
	}
	
}

class NoteBook extends Product {
	NoteBook(int price) {
		super(150);
	}
	
	@Override
	public String toString() {
		return "NoteBook";
	}
	
}



