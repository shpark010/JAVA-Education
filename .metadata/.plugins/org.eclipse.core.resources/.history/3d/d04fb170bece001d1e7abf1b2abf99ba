
class Product {
	int price;
	int bonuspoint;
	
	Product(int price) {
		this.price = price;
		this.bonuspoint = (int)(this.price/10.0);
	}
}

class KtTv extends Product {
	String name;
	
	public KtTv(String name, int price) {
		super(price);
	}
}



