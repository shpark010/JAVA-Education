import java.util.Scanner;

public class Solution {
    static int AnswerN;
    static int N = 10;

    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
       
       int T = sc.nextInt();
       
       for(int test_case = 1 ; test_case <= T ; test_case++) {
    	   N = sc.nextInt();
    	   System.out.println("#" + test_case + ' ' + AnswerN);
       }
    	
    	
    }
}